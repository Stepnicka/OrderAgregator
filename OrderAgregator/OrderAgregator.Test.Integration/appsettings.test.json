{
  "ConnectionStrings": {
    "SqLite": "Data source=test"
  },
  "SqLite": {
    "RebuildOnInitialize": true
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "RateLimit": {
    "Seconds": 5
  },
  "AllowedHosts": "*"
}
